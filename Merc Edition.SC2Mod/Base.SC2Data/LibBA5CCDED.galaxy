//--------------------------------------------------------------------------------------------------
// Trigger: Hill Respawn
//--------------------------------------------------------------------------------------------------
bool libBA5CCDED_gt_HillRespawn_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unit lv_hill;

    // Automatic Variable Declarations
    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!((UnitGetType(EventUnit()) == "GravenHill"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    lv_hill = EventUnit();
    libNtve_gf_MakeUnitInvulnerable(lv_hill, true);
    libNtve_gf_MakeUnitUncommandable(lv_hill, true);
    UnitSetPosition(lv_hill, libBA5CCDED_gv_hillRespawnPoint, false);
    UnitBehaviorAdd(lv_hill, "Hill180sRespawn", lv_hill, 1);
    Wait(180.0, c_timeGame);
    libNtve_gf_MakeUnitInvulnerable(lv_hill, false);
    libNtve_gf_MakeUnitUncommandable(lv_hill, false);
    return true;
}

